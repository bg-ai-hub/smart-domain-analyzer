}{
  "Domain": "Ordering",
  "Subdomains": [
    {
      "name": "PaymentGateway",
      "BoundedContexts": [
        {
          "name": "PaymentMethods",
          "Aggregates": [
            {
              "name": "UPIPayment",
              "Entities": [
                {
                  "name": "UPIPaymentDetails",
                  "Attributes": [
                    {
                      "name": "account_number",
                      "data_type": "string",
                      "description": "User's account number"
                    },
                    {
                      "name": "amount",
                      "data_type": "decimal",
                      "description": "Amount to be paid"
                    }
                  ]
                }
              ]
            },
            {
              "name": "CreditCardPayment",
              "Entities": [
                {
                  "name": "CreditCardDetails",
                  "Attributes": [
                    {
                      "name": "card_number",
                      "data_type": "string",
                      "description": "User's credit card number"
                    },
                    {
                      "name": "expiration_date",
                      "data_type": "date",
                      "description": "Credit card expiration date"
                    }
                  ]
                }
              ]
            }
          ]
        }
      ]
    },
    {
      "name": "OrderManagement",
      "BoundedContexts": [
        {
          "name": "OrderPlacement",
          "Aggregates": [
            {
              "name": "OrderDetails",
              "Entities": [
                {
                  "name": "MenuItem",
                  "Attributes": [
                    {
                      "name": "id",
                      "data_type": "integer",
                      "description": "Unique identifier for the menu item"
                    },
                    {
                      "name": "name",
                      "data_type": "string",
                      "description": "Name of the menu item"
                    }
                  ]
                },
                {
                  "name": "OrderItem",
                  "Attributes": [
                    {
                      "name": "quantity",
                      "data_type": "integer",
                      "description": "Quantity of the order item"
                    },
                    {
                      "name": "price",
                      "data_type": "decimal",
                      "description": "Price of the order item"
                    }
                  ]
                }
              ]
            }
          ]
        },
        {
          "name": "OrderTracking",
          "Aggregates": [
            {
              "name": "OrderStatus",
              "Entities": [
                {
                  "name": "OrderStatusDetails",
                  "Attributes": [
                    {
                      "name": "status",
                      "data_type": "string",
                      "description": "Current status of the order"
                    },
                    {
                      "name": "timestamp",
                      "data_type": "datetime",
                      "description": "Timestamp when the status was updated"
                    }
                  ]
                }
              ]
            }
          ]
        }
      ]
    },
    {
      "name": "DeliveryPartnerModule",
      "BoundedContexts": [
        {
          "name": "DeliveryTracking",
          "Aggregates": [
            {
              "name": "DeliveryStatus",
              "Entities": [
                {
                  "name": "DeliveryStatusDetails",
                  "Attributes": [
                    {
                      "name": "status",
                      "data_type": "string",
                      "description": "Current status of the delivery"
                    },
                    {
                      "name": "timestamp",
                      "data_type": "datetime",
                      "description": "Timestamp when the status was updated"
                    }
                  ]
                }
              ]
            }
          ]
        }
      ]
    },
    {
      "name": "ReviewAndRating",
      "BoundedContexts": [
        {
          "name": "RestaurantReviews",
          "Aggregates": [
            {
              "name": "RestaurantReview",
              "Entities": [
                {
                  "name": "ReviewDetails",
                  "Attributes": [
                    {
                      "name": "rating",
                      "data_type": "integer",
                      "description": "Rating given by the user"
                    },
                    {
                      "name": "review_text",
                      "data_type": "string",
                      "description": "Text of the review"
                    }
                  ]
                }
              ]
            }
          ]
        }
      ]
    }
  ]
}